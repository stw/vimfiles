setlocal omnifunc=
setlocal path=/etc/perl,/usr/local/lib/perl/5.10.0,/usr/local/share/perl/5.10.0,/usr/lib/perl5,/usr/share/perl5,/usr/lib/perl/5.10,/usr/share/perl/5.10,/usr/local/lib/site_perl,,
setlocal nopreserveindent
